{% extends 'base.html.twig' %}

{% block body %}
    <h1>Clubstats list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Label</th>
                <th>Shortlabel</th>
                <th>Value</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for clubStat in clubStats %}
            <tr>
                <td><a href="{{ path('clubstat_show', { 'id': clubStat.id }) }}">{{ clubStat.id }}</a></td>
                <td>{{ clubStat.label }}</td>
                <td>{{ clubStat.shortLabel }}</td>
                <td>{{ clubStat.value }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('clubstat_show', { 'id': clubStat.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('clubstat_edit', { 'id': clubStat.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('clubstat_new') }}">Create a new clubStat</a>
        </li>
    </ul>
{% endblock %}
